@model List<ApplicantVM>
@{
    ViewData["Title"] = "Interview";
}

<div class="container">
    <div class="row" id="applicants" style="margin-top: 30px;">
        @for (int i = 0; i < Model.Count(); i++)
        {
            <div class="col-sm-6 col-lg-4 d-flex">
                <partial name="../Interviewer/Partials/_Applicants" model="@Model[i]" />
            </div>
        }
    </div>
</div>
