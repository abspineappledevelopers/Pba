@model List<UCL.ISM.Client.Models.InterviewSchemeVM>
@{
    ViewData["Title"] = "Get_All_InterviewSchemes_And_Questions";
}

<div style="padding: 10px 7px 40px 7px; background-image: linear-gradient(to bottom, #cbe5e4, #edf9fc);">
    <div class="text-center" style="margin-top: 40px; padding: 10px 7px 10px 7px">
        <h4 style="color: #014550">Alle interview skemaer</h4>
    </div>

    <div class="row">
        @for (int i = 0; i < Model.Count; i++)
        {
            <div class="col-3">
                <partial name="../Administration/Partials/_EditSchemeAndQuestions" model="Model[i]" />
            </div>
        }
    </div>
</div>

